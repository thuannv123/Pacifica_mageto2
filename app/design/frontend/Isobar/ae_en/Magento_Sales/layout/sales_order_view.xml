<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="breadcrumbs" template="Magento_Theme::html/breadcrumbs.phtml">
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">home</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Home</item>
                    <item name="label" xsi:type="string">Home</item>
                    <item name="link" xsi:type="string">/</item>
                </argument>
            </action>
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">my-newadd</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Order List</item>
                    <item name="label" xsi:type="string">Order List</item>
                </argument>
            </action>
        </referenceBlock>
<!--        <referenceBlock name="order.status" remove="true" />-->
<!--        <referenceBlock name="sales.order.info" remove="true" />-->
    </body>
</page>

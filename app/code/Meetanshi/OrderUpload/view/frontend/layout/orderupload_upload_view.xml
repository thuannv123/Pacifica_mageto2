<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Order Attachments</title>
        <css src="Meetanshi_OrderUpload::css/orderupload.css"/>
    </head>
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Meetanshi\OrderUpload\Block\OrderUploads" name="orderuploadlist" template="Meetanshi_OrderUpload::orderuploadlist.phtml" ifconfig="orderupload/general/enabled" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
